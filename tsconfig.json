{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../out-tsc/e2e",
    "module": "commonjs",
    "target": "es2018",
    "types": [
      "node",
      "webdriverio/async",
      "@wdio/mocha-framework"
    ],
    "paths": {
      "@test/*": ["./test/*"],
      "@enums/*": ["./test/enums/*"],
      "@pages/*": ["./test/pages/*"],
      "@services/*": ["./test/services/*"],
      "@type/*": ["./test/types/*"],
    }
  }
}
